<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Tree View -->
    <record id="view_history_source_account_tree" model="ir.ui.view">
        <field name="name">history.source.account.tree</field>
        <field name="model">history.source.account</field>
        <field name="arch" type="xml">
            <list>
                <field name="name"/>
                <field name="date_start"/>
                <field name="date_end"/>
                <field name="currency_id"/>
            </list>
        </field>
    </record>

    <!-- Form View -->
    <record id="view_history_source_account_form" model="ir.ui.view">
        <field name="name">history.source.account.form</field>
        <field name="model">history.source.account</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="date_start"/>
                        <field name="date_end"/>
                        <field name="currency_id"/>
                        <field name="code"/>
                    </group>
                    <notebook>
                        <page string="Notes">
                            <field name="notes"/>
                        </page>
                    </notebook>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
