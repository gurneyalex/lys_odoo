<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Form View -->
    <record id="view_history_clothing_caracteristic_form" model="ir.ui.view">
        <field name="name">history.clothing.caracteristic.form</field>
        <field name="model">history.clothing.caracteristic</field>
        <field name="arch" type="xml">
            <form string="Clothing Caracteristic">
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <!-- List View -->
    <record id="view_history_clothing_caracteristic_list" model="ir.ui.view">
        <field name="name">history.clothing.caracteristic.list</field>
        <field name="model">history.clothing.caracteristic</field>
        <field name="arch" type="xml">
            <list string="Clothing Caracteristics">
                <field name="name"/>
            </list>
        </field>
    </record>
</odoo>
